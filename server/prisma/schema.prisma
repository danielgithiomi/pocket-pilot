generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum AccountType {
  SAVINGS
  CURRENT
}

model Account {
  id     String      @id @default(uuid())
  holder String      @unique
  type   AccountType @default(CURRENT)
}
